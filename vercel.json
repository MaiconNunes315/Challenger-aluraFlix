{
    "version": 2,
    "builds": [
        {
            "src": "./public/data",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "initial-data.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}